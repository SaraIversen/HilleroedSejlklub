@page
@using SejlklubLibrary.Models;
@model Razor.Pages.Members.ShowAdministratorsModel
@{
	ViewData["Title"] = "Show Administrators";
}

<table class="table">
	<thead>
		<tr>
			<th>ID</th>
			<th>Navn</th>
			<th>Addresse</th>
			<th>Email</th>
			<th>Telefon</th>
			<th>Medlems Status</th>
			<th>Titel</th>
			<th>Billed</th>
		</tr>
	</thead>
	<tbody>
		@foreach (Administrator item in Model.Administrators)
		{
			<tr>
				<td>@item.Id</td>
				<td>@item.Name</td>
				<td>@item.Address</td>
				<td>@item.Email</td>
				<td>@item.Phone</td>
				<td>@item.MemberStatus</td>
				<td>@item.Title</td>
				<td>
					<img src="/images/memberImages/@item.MemberImage" class="rounded-circle" width="80" height="70" />
				</td>
			</tr>
		}
	</tbody>
</table>
