@page
@model Razor.Pages.Members.EditMembersModel
@{
	ViewData["Title"] = "Edit Members";
}

<form method="post">
	<div class="form-group">
		<label asp-for="@Model.Member.Id" class="control-label"></label>
		<input asp-for="@Model.Member.Id" readonly class="form-control" />
	</div>
	<div class="form-group">
		<label asp-for="@Model.Member.Name" class="control-label"></label>
		<input asp-for="@Model.Member.Name" class="form-control" />
	</div>
	<div class="form-group">
		<label asp-for="@Model.Member.Email" class="control-label"></label>
		<input asp-for="@Model.Member.Email" class="form-control" />
	</div>
	<div class="form-group">
		<label asp-for="@Model.Member.Address" class="control-label"></label>
		<input asp-for="@Model.Member.Address" class="form-control" />
	</div>
	<div class="form-group">
		<label asp-for="@Model.Member.Phone" class="control-label"></label>
		<input asp-for="@Model.Member.Phone" class="form-control" />
	</div>
	<div class="formgroup">
		<input type="submit" value="Ændre Medlem" class="btn btn-primary" />
	</div>
</form>
